        <!-- Form -->
        <form action="/signup" method="POST" style="padding: 1.5rem;">
            <!-- Form headlines -->
            <h5 class="form-branding">S-Manager</h5>
            <h4 class="form-name">Create Your Account</h4>
            <!-- displaying warnings -->
            <% if (locals.messages.warning) { %>
            <div class="form-alert alert-warning">
                <span class="closeBtn" onclick="alertClose(this)">&times;</span>
                <%=locals.messages.warning%>
            </div>
            <% } %>
            <!-- displaying error messages -->
            <% if (locals.messages.err) { %>
            <div class="form-alert">
                <span class="closeBtn" onclick="alertClose(this)">&times;</span>
                <%=locals.messages.err%>
            </div>
            <% } %>
            <!-- form single email input -->
            <div class="form-input-group">
                <label>Your Name</label>
                <input class="form-input" type="text" id="name" name="name" placeholder="Name">
            </div>
            <!-- form single email input -->
            <div class="form-input-group">
                <label>Email Address</label>
                <input class="form-input" inputmode="email" type="email" id="email" name="email" placeholder="Email">
            </div>
            <!-- form single password input -->
            <div class="form-input-group">
                <label>Password</label>
                <input class="form-input" type="password" id="password" name="password" placeholder="Password">
            </div>
            <!-- form single conform password input -->
            <div class="form-input-group">
                <label>Conform Password</label>
                <input class="form-input" type="password" id="c-password" name="cpassword"
                    placeholder="Conform Password">
            </div>
            <!-- form input button -->
            <input class="form-button" type="submit" value="Submit">
            <!-- form splitter -->
            <div class="b111">
                <div class="b112"></div>
                <p class="b113">or</p>
            </div>
            <!-- Form oAuth section -->
            <div class="form-oauth">
                <a class="oauth-anchor" href="/auth/github">
